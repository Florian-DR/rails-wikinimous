<h1>Articles#edit</h1>
<p>Find me in app/views/articles/edit.html.erb</p>
<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>

<%= link_to "⬅️ Back to the article", article_path(@article) %>
